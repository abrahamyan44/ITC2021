{"ast":null,"code":"export const simpleCard = (dark, type) => {\n  const darkProp = dark ? ' dark' : '';\n  return `<Card${darkProp}${type ? ' ' + type : ''}>\n  <CardContent>\n    <Subtitle2 secondary style={{ marginBottom: '4px' }} >\n      Word of the day\n    </Subtitle2>\n    <H5>\n      be<span>•</span>nev<span>•</span>o<span>•</span>lent\n    </H5>\n    <Subtitle2 secondary style={{ marginBottom: '12px' }} >\n      adjective\n    </Subtitle2>\n    <Body2>\n      well meaning and kindly.\n      <br />\n      \"a benevolent smile\"\n    </Body2>\n  </CardContent>\n  <CardAction>\n    <Button text color='var(--primary)'>\n      Learn More\n    </Button>\n  </CardAction>\n</Card>`;\n};\nexport const simpleCardCopy = (dark, type, data) => {\n  const darkProp = dark ? ' dark' : '';\n  return `<Card${darkProp}${type ? ' ' + type : ''}>\n  ${data || '...'}\n</Card>`;\n};\nexport const elevationCard = dark => {\n  return `${[0, 1, 2, 3, 4, 5].map(i => simpleCardCopy(dark, `elevation={${i}} width={100} height={100}`, String(i))).join('\\n')}`;\n};\nexport const mediaCard = dark => {\n  const darkProp = dark ? ' dark' : '';\n  return `<Card${darkProp}>\n  <CardMedia\n    dark\n    src='images/beaches-2.jpg'\n    title='Top 10 Indian beaches'\n  />\n  <CardContent>\n    <Subtitle2 secondary style={{ marginBottom: '4px' }} >\n      Number 1\n    </Subtitle2>\n    <Body2>\n      Radhanagar Beach <br />\n      Havock Island, Andaman\n    </Body2>\n  </CardContent>\n  <CardAction>\n    <Button text color='var(--primary)'>\n      Share\n    </Button>\n    <Button text color='var(--primary)'>\n      Explore\n    </Button>\n  </CardAction>\n</Card>`;\n};\nexport const complexCard = dark => {\n  const darkProp = dark ? ' dark' : '';\n  return `<Card${darkProp}>\n  <CardHeader\n    title={<H6>Lorem ipsum</H6>}\n    subtitle={<Subtitle2 secondary>Lorem ipsum dolor sit amet</Subtitle2>}\n    action={\n      <IconButton>\n        <Icon path={mdiDotsVertical} size={1}></Icon>\n      </IconButton>\n    }\n  />\n  <CardMedia dark src='images/beaches-2.jpg' />\n  <CardContent>\n    <Body2>\n      Lorem ipsum dolor sit amet consectetur, adipisicing elit. Atque\n      architecto reprehenderit magnam esse est id ipsum ut delectus.\n      Consequuntur suscipit hic eum ea adipisci, illum sed iure saepe\n      aperiam quia!\n    </Body2>\n  </CardContent>\n  <CardAction>\n    <Button text color='var(--primary)'>\n      Read\n    </Button>\n    <Button text color='var(--primary)'>\n      Bookmark\n    </Button>\n    <Spacer />\n    <IconButton>\n      <Icon path={mdiHeart} size={1}></Icon>\n    </IconButton>\n    <IconButton>\n      <Icon path={mdiShareVariant} size={1}></Icon>\n    </IconButton>\n  </CardAction>\n</Card>`;\n};","map":{"version":3,"sources":["/home/yeva/Desktop/New Folder/health-app/src/docs/code/card-code.js"],"names":["simpleCard","dark","type","darkProp","simpleCardCopy","data","elevationCard","map","i","String","join","mediaCard","complexCard"],"mappings":"AAAA,OAAO,MAAMA,UAAU,GAAG,CAACC,IAAD,EAAOC,IAAP,KAAgB;AACxC,QAAMC,QAAQ,GAAGF,IAAI,GAAG,OAAH,GAAa,EAAlC;AACA,SAAQ,QAAOE,QAAS,GAAED,IAAI,GAAG,MAAIA,IAAP,GAAY,EAAG;AAC/C;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,QAtBE;AAuBD,CAzBM;AA2BP,OAAO,MAAME,cAAc,GAAG,CAACH,IAAD,EAAOC,IAAP,EAAaG,IAAb,KAAsB;AAClD,QAAMF,QAAQ,GAAGF,IAAI,GAAG,OAAH,GAAa,EAAlC;AACA,SAAQ,QAAOE,QAAS,GAAED,IAAI,GAAG,MAAIA,IAAP,GAAY,EAAG;AAC/C,IAAIG,IAAI,IAAI,KAAM;AAClB,QAFE;AAGD,CALM;AAOP,OAAO,MAAMC,aAAa,GAAIL,IAAD,IAAU;AACrC,SAAQ,GAAE,CAAC,CAAD,EAAI,CAAJ,EAAO,CAAP,EAAU,CAAV,EAAa,CAAb,EAAgB,CAAhB,EACPM,GADO,CACFC,CAAD,IACHJ,cAAc,CACZH,IADY,EAEX,cAAaO,CAAE,4BAFJ,EAGZC,MAAM,CAACD,CAAD,CAHM,CAFR,EAQPE,IARO,CAQF,IARE,CAQI,EARd;AASD,CAVM;AAYP,OAAO,MAAMC,SAAS,GAAIV,IAAD,IAAU;AACjC,QAAME,QAAQ,GAAGF,IAAI,GAAG,OAAH,GAAa,EAAlC;AACA,SAAQ,QAAOE,QAAS;AAC1B;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,QAvBE;AA